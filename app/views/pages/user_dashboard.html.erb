<div class="row align-items-center justify-content-center dashboard_container py-2">
  <ul class="nav nav-pills " id="pills-tab" role="tablist">
    <li class="nav-item">
      <a class="nav-link active" id="pills-home-tab" data-toggle="pill" href="#pills-home" role="tab" aria-controls="pills-home" aria-selected="true">Personal Information</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" id="pills-creation-tab" data-toggle="pill" href="#pills-creation" role="tab" aria-controls="pills-profile" aria-selected="false">Applications</a>
    </li>
    <li class="nav-item">
      <a class="nav-link" id="pills-event-tab" data-toggle="pill" href="#pills-event" role="tab" aria-controls="pills-event" aria-selected="false">Programs you've liked</a>
    </li>
  </ul>
</div>

<div class="section">
  <div class="container">
    <div class="row">
      <div class="col">
        <% @applications.each do |application| %>
          <%#= render partial: 'shared/program_card_index', locals: {program: program} %>
          <%= application.status %>
        <% end %>
      </div>
    </div>
  </div>
</div>
<div class="section">
  <div class="container">
    <div class="row">
      <div class="col">
        <% @favorite_programs.each do |program| %>
          <%= render partial: 'shared/program_card_index', locals: {program: program} %>
        <% end %>
      </div>
    </div>
  </div>
</div>
